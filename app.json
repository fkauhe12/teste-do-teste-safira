{
  "expo": {
    "name": "testessafira",
    "slug": "testessafira",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fkauhe12.testessafira",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "UIViewControllerBasedStatusBarAppearance": false,
        "NSUserTrackingUsageDescription": "Este app precisa rastrear dados para fornecer notificações personalizadas."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.fkauhe12.testessafira",
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification-icon.png",
          "color": "#4873FF"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "bf7c6868-4fc2-47e5-b410-0eff22398a5b"
      }
    },
    "owner": "fkauhe12",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bf7c6868-4fc2-47e5-b410-0eff22398a5b"
    }
  }
}
